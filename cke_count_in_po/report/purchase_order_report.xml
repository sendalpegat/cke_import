<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="no_of_quantity_purchase_report" inherit_id="purchase.report_purchaseorder_document">
            <xpath expr="//div[@class='row justify-content-end']" position="after">
               <div style="padding-top:20px; padding-bottom:20px;">
                   <p><span><strong>No. Ordered Quantity: </strong></span><span t-field="o.ordered_quantity"/></p>
                   <p><span><strong>No. Received Quantity: </strong></span><span t-field="o.received_quantity"/></p>
                   <p><span><strong>No. Billed Quantity: </strong></span><span t-field="o.biled_quantity"/></p>
                </div>
            </xpath>
        </template>
</odoo>
