<odoo>
    <record model="ir.ui.view" id="stock_picking_delivery_status_stamp_form_view">
        <field name="name">stock.picking.form.view</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Receipt Time Stamp" attrs="{'invisible':[('picking_type_code', '!=', 'incoming')]}">
                    <field name="delivery_status_stamp_ids">
                        <tree string="Status Stamp" delete="false" create="false">
                            <field name="purchase_id" invisible="1"/>
                            <field name="picking_id" invisible="1"/>
                            <field name="delivery_status"/>
                            <field name="create_date"/>
                            <field name="create_uid"/>
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>