<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="no_of_quantity_delivery_report" inherit_id="stock.report_picking">
            <xpath expr="//div[hasclass('page')]" position="inside">
                <div class="row">
                    <div class="col-md-6">
                        <p><span><strong>No. Demand Quantity: </strong></span><span t-field="o.demand_quantity"/></p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <p><span><strong>No. Done Quantity: </strong></span><span t-field="o.done_quantity"/></p>
                    </div>
                </div>
            </xpath>
        </template>
</odoo>
