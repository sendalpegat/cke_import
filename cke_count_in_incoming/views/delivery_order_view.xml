<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_delivery_order_form" model="ir.ui.view">
        <field name="name">no of order quantity</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="after">
                <group colspan="2">
                    <group>
                        <field name="demand_quantity" attrs="{'invisible':[('picking_type_code', '!=', 'incoming')]}"/>
                        <field name="done_quantity" attrs="{'invisible':[('picking_type_code', '!=', 'incoming')]}"/>
                    </group>
                </group>
            </xpath>
        </field>
    </record>
</odoo>

