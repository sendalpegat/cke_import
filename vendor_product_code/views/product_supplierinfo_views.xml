<odoo>
    <record id="view_product_supplierinfo_form" model="ir.ui.view">
        <field name="name">product.supplierinfo.form</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.view_product_supplierinfo_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='min_qty']" position="after">
                <field name="vendor_product_code"/>
            </xpath>
        </field>
    </record>
</odoo>