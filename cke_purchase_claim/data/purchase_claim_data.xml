<odoo>
    <record id="purchase_claim_sequence" model="ir.sequence">
        <field name="name">Purchase Claim Sequence</field>
        <field name="code">purchase.claim</field>
        <field name="prefix">PC-</field>
        <field name="padding">5</field>
    </record>

    <record id="purchase_claim_sequence_data" model="ir.sequence.date_range">
        <field name="sequence_id" ref="purchase_claim_sequence"/>
        <field name="date_from">2023-01-01</field>
        <field name="date_to">2023-12-31</field>
        <field name="number_next">1</field>
    </record>
</odoo>